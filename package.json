{
  "name": "api-graphql",
  "version": "1.0.0",
  "main": "dist/main.js",
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsnd -r tsconfig-paths/register --transpile-only --clear --ignore-watch node_modules src/main.ts",
    "prod": "run-s clean compile run",
    "test": "cross-env NODE_ENV=testing jest --detectOpenHandles",
    "test:watch": "yarn test --watchAll",
    "clean": "rimraf dist",
    "compile": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "run": "cross-env NODE_ENV=production node dist/main.js",
    "typeorm": "tsnd -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "lint": "eslint ./ --ext js,ts",
    "lint:fix": "eslint ./ --ext js,ts --fix",
    "prettier": "prettier --check ./**/*.{js,ts}",
    "prettier:fix": "prettier --write ./**/*.{js,ts}"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "apollo-server": "^2.19.2",
    "apollo-server-testing": "^2.19.2",
    "async": "^3.2.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.16.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "handlebars": "^4.7.6",
    "ioredis": "^4.19.4",
    "jsonwebtoken": "^8.5.1",
    "jw-paginate": "^1.0.4",
    "merge-graphql-schemas": "^1.7.8",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.17",
    "pg": "^8.5.1",
    "rate-limiter-flexible": "^2.2.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^1.1.1",
    "tsyringe": "^4.4.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.30",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/plugin-proposal-decorators": "7.14.2",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-typescript": "7.13.0",
    "@commitlint/cli": "12.1.4",
    "@types/async": "3.2.6",
    "@types/bcryptjs": "2.4.2",
    "@types/dotenv": "8.2.0",
    "@types/ioredis": "4.26.4",
    "@types/jest": "26.0.23",
    "@types/jsonwebtoken": "8.5.1",
    "@types/node": "14.17.1",
    "@types/nodemailer": "6.4.1",
    "@types/redis": "2.8.28",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-parameter-decorator": "1.0.16",
    "babel-plugin-transform-typescript-metadata": "0.3.2",
    "cross-env": "7.0.3",
    "eslint": "7.27.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-import-helpers": "1.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.3.2"
  }
}
